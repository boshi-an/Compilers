decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()

global @x = alloc i32, 0
global @y = alloc i32, 10
global @z = alloc i32, 11
global @init = alloc i32, 1
global @init2 = alloc i32, 2
global @init3 = alloc i32, 1
global @init4 = alloc i32, 1
global @init5 = alloc i32, 3
global @init6 = alloc i32, 2
fun  @main(): i32 {
%entry: 
    %1 = load @x
    call @putint(%1)

    %2 = add 0, 32
    call @putch(%2)

    %3 = add 0, 10
    call @putint(%3)

    %4 = add 0, 32
    call @putch(%4)

    %5 = add 0, 11
    call @putint(%5)

    %6 = add 0, 32
    call @putch(%6)

    %7 = load @init
    call @putint(%7)

    %8 = add 0, 10
    call @putch(%8)

    %9 = add 0, 0
    ret %9

}
